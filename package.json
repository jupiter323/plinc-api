{
  "name": "plinc-api",
  "version": "1.0.0",
  "description": "API for Plinc.app",
  "main": "index.js",
  "scripts": {
    "start": "sam local start-api -t .ci/service.yml",
    "integration": "tape src/**/*.integration.test.js",
    "api:validate": "npm run swagger:bundle && npm run swagger:convert && swagger-cli validate ./swagger/spec.json",
    "swagger:bundle": "swagger-cli bundle .ci/spec/spec.yml -o .ci/spec/bundle.spec.json",
    "swagger:convert": "cat .ci/spec/bundle.spec.json | jq '.info.title = \"Plinc API\"' > swagger/spec.json",
    "swagger:start": "webpack-dev-server --open --inline --mode development",
    "swagger:build": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Morriphi/plinc-api.git"
  },
  "author": "Philip Morris",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Morriphi/plinc-api/issues"
  },
  "homepage": "https://github.com/Morriphi/plinc-api#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^3.0.1",
    "request": "^2.88.0",
    "style-loader": "^0.23.1",
    "swagger-cli": "^2.3.0",
    "tape": "^4.11.0",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "aws-sdk": "^2.493.0",
    "jwt-claims": "^1.0.1",
    "swagger-ui": "^3.23.1",
    "uuid": "3.3.2"
  },
  "files": [
    "/src/**/*.js",
    "/src/**/*.json",
    "!/src/**/*.test.js",
    "!/src/**/*.integration.js"
  ]
}
